{
  "name": "music-you",
  "version": "2.1.0",
  "private": true,
  "author": "gumengyu",
  "description": "A NetEaseCloudMusic third-party music player (base on vue@3 and vuetify)",
  "scripts": {
    "preview": "vite preview",
    "dev": "vite",
    "build": "vite build",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:ts",
    "lint:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx,vue}\" --fix",
    "lint:prettier": "prettier --write \"src/**/*.{js,tsx,css,scss,vue,md,json,html}\"",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "dev:https": "vite build && npm run serve",
    "serve": "serve dist",
    "tauri": "tauri",
    "client:build:mac_arm": "npm run tauri build --target aarch64-apple-darwin",
    "client:build:mac_x86": "npm run tauri build --target x86_64-apple-darwin",
    "client:build:mac_universal": "npm run tauri build --target universal-apple-darwin"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.2.0",
    "NeteaseCloudMusicApi": "git+https://github.com/GuMengYu/NeteaseCloudMusicApi.git#v-player",
    "axios": "^1.2.6",
    "dotenv": "^16.0.1",
    "http-proxy-middleware": "^2.0.6",
    "polka": "^0.5.2",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@material/material-color-utilities": "^0.2.0",
    "@mdi/js": "^6.5.95",
    "@tauri-apps/cli": "^1.2.2",
    "@types/howler": "^2.2.7",
    "@types/lodash-es": "^4.17.6",
    "@types/md5": "^2.3.2",
    "@types/overlayscrollbars": "^1.12.1",
    "@types/polka": "^0.5.4",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vueuse/components": "9.8.1",
    "@vueuse/core": "9.8.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.7.0",
    "execa": "^6.1.0",
    "howler": "^2.2.3",
    "https-localhost": "^4.7.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.9.2",
    "md5": "^2.3.0",
    "ora": "^6.1.2",
    "overlayscrollbars": "^1.13.1",
    "pinia": "^2.0.13",
    "pkg": "^5.8.0",
    "plyr": "^3.7.2",
    "prettier": "^2.6.2",
    "sass": "^1.38.0",
    "stackblur-canvas": "^2.5.0",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.3",
    "vite-plugin-pwa": "^0.14.1",
    "vite-plugin-resolve": "^2.3.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.2.41",
    "vue-content-loader": "^2.0.1",
    "vue-i18n": "^9.1.9",
    "vue-kinesis": "^2.0.5",
    "vue-router": "^4.0.14",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-tsc": "^1.0.8",
    "vue-virtual-scroller": "^2.0.0-alpha.1",
    "vue3-slider": "git+https://github.com/GuMengYu/vue3-slider.git",
    "vuetify": "^3.1.2",
    "vuetify-ctx-menu": "git+https://github.com/GuMengYu/vue3-context-menu.git#v0.0.1",
    "webfontloader": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
